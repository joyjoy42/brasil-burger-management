{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=BrasilBurger.db"
  },

  "Cloudinary": {
    "CloudName": "VOTRE_CLOUD_NAME",
    "ApiKey": "VOTRE_API_KEY",
    "ApiSecret": "VOTRE_API_SECRET"
  },

  "Logging": {
    "LogLevel": {
